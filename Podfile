# Uncomment the next line to define a global platform for your project
platform :ios, '12.1'

target 'Cheeky Radio Pro' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'MediaPlayer', :podspec => "venders/MediaPlayer.podspec"
  pod 'Foundation', :podspec => "venders/Foundation.podspec"
  pod 'UIKit', :podspec => "venders/UIKit.podspec"
  pod 'MessageUI', :podspec => "venders/MessageUI.podspec"
  pod 'AudioToolbox', :podspec => "venders/AudioToolbox.podspec"
  pod 'AVFoundation', :podspec => "venders/AVFoundation.podspec"
  pod 'XCTest', :podspec => "venders/XCTest.podspec"
  pod 'SwinShims', :podspec => "venders/SwinShims.podspec"

  # Pods for Cheeky Radio Pro

  target 'Cheeky Radio ProUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
